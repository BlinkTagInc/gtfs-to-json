{
  "name": "gtfs-to-routes",
  "version": "3.4.3",
  "private": false,
  "description": "Generate JSON of transit route data from a GTFS file",
  "keywords": [
    "transit",
    "gtfs",
    "transportation"
  ],
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "type": "module",
  "main": "index.js",
  "bin": {
    "gtfs-to-routes": "bin/gtfs-to-routes.js"
  },
  "scripts": {
    "prepare": "husky"
  },
  "dependencies": {
    "cli-table": "^0.3.11",
    "gtfs": "^4.13.4",
    "lodash-es": "^4.17.21",
    "pretty-error": "^4.0.0",
    "sanitize-filename": "^1.6.3",
    "timer-machine": "^1.1.0",
    "yargs": "^17.7.2",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": "prettier --write",
    "*.json": "prettier --write"
  }
}
